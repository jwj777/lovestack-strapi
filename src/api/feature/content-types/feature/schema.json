{
  "kind": "collectionType",
  "collectionName": "features",
  "info": {
    "singularName": "feature",
    "pluralName": "features",
    "displayName": "Feature",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "featureName": {
      "type": "string"
    },
    "companies": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::company.company",
      "mappedBy": "features"
    },
    "product_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product-category.product-category",
      "inversedBy": "features"
    },
    "featureDescription": {
      "type": "richtext"
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "Weight": {
      "type": "integer",
      "max": 50
    },
    "feature_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::feature-category.feature-category",
      "inversedBy": "features"
    }
  }
}
