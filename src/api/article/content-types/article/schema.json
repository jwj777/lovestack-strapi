{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Headline": {
      "type": "string"
    },
    "Subheading": {
      "type": "richtext"
    },
    "slug": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "featureImage": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "body": {
      "type": "customField",
      "options": {
        "preset": "rich",
        "maxLengthCharacters": 100000
      },
      "customField": "plugin::ckeditor.CKEditor"
    },
    "pageTitle": {
      "type": "string"
    },
    "metaDescription": {
      "type": "text"
    },
    "summaryImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "titleLabel": {
      "type": "string"
    },
    "isLive": {
      "type": "boolean"
    }
  }
}
