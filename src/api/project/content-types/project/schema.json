{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Project",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "titleLabel": {
      "type": "string"
    },
    "Headline": {
      "type": "string"
    },
    "Subheading": {
      "type": "text"
    },
    "slug": {
      "type": "string"
    },
    "sections": {
      "type": "dynamiczone",
      "components": [
        "section.divider",
        "section.image-series-sm",
        "section.section-big-text",
        "section.text-cards",
        "section.text-two-col",
        "section.section-two-column",
        "section.big-image",
        "section.project-images",
        "section.basic-text-ck"
      ]
    },
    "industries": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::industry.industry",
      "inversedBy": "projects"
    },
    "hexOne": {
      "type": "string"
    },
    "hexTwo": {
      "type": "string"
    },
    "hexThree": {
      "type": "string"
    },
    "summaryImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "pageTitle": {
      "type": "string"
    },
    "metaDescription": {
      "type": "text"
    },
    "projectCategories": {
      "type": "enumeration",
      "enum": [
        "Design",
        "Branding",
        "Web Development",
        "SEO"
      ]
    },
    "projectUrl": {
      "type": "string"
    },
    "color": {
      "type": "enumeration",
      "enum": [
        "neutralWhite",
        "neutralLight",
        "neutralDark",
        "neutralVariantWhite",
        "neutralVariantLight",
        "neutralVariantDark",
        "primaryWhite",
        "primaryLight",
        "primaryDark",
        "secondaryWhite",
        "secondaryLight",
        "secondaryDark",
        "tertiaryWhite",
        "tertiaryLight",
        "tertiaryDark"
      ]
    }
  }
}
