{
  "kind": "collectionType",
  "collectionName": "stacks",
  "info": {
    "singularName": "stack",
    "pluralName": "stacks",
    "displayName": "stack",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "stackName": {
      "type": "string"
    },
    "stackItem": {
      "displayName": "Stack Item",
      "type": "component",
      "repeatable": true,
      "component": "software.stack-item"
    },
    "slug": {
      "type": "string"
    },
    "featureImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "stackAnalysis": {
      "type": "customField",
      "options": {
        "preset": "rich"
      },
      "customField": "plugin::ckeditor.CKEditor"
    },
    "stackDescription": {
      "type": "customField",
      "options": {
        "preset": "rich",
        "maxLengthCharacters": 200
      },
      "customField": "plugin::ckeditor.CKEditor"
    },
    "authorName": {
      "type": "string"
    },
    "authorTitle": {
      "type": "string"
    },
    "authorCompany": {
      "type": "string"
    },
    "authorWebsite": {
      "type": "string"
    },
    "stackSummary": {
      "type": "customField",
      "options": {
        "preset": "rich",
        "maxLengthCharacters": 180
      },
      "customField": "plugin::ckeditor.CKEditor"
    }
  }
}
