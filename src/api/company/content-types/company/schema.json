{
  "kind": "collectionType",
  "collectionName": "companies",
  "info": {
    "singularName": "company",
    "pluralName": "companies",
    "displayName": "Company",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "companyName": {
      "type": "string"
    },
    "companyDescription": {
      "type": "richtext"
    },
    "companyUrl": {
      "type": "string"
    },
    "product_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product-category.product-category",
      "inversedBy": "companies"
    },
    "slug": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "twitterHandle": {
      "type": "string",
      "unique": false
    },
    "linkedinHandle": {
      "type": "string"
    },
    "parentCompany": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::company.company",
      "inversedBy": "subsidiaries"
    },
    "subsidiaries": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::company.company",
      "mappedBy": "parentCompany"
    },
    "features": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::feature.feature",
      "inversedBy": "companies"
    },
    "entryPriceMonthly": {
      "type": "decimal"
    },
    "yearFounded": {
      "type": "integer"
    },
    "Headquarters": {
      "type": "string"
    },
    "webScreenshot": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "freeTrial": {
      "type": "boolean"
    },
    "freePricingTier": {
      "type": "boolean"
    },
    "integrationsPage": {
      "type": "string"
    },
    "weight": {
      "type": "integer"
    },
    "featured": {
      "type": "boolean",
      "default": false
    },
    "authorityRank": {
      "type": "decimal"
    },
    "pricingPage": {
      "type": "string"
    },
    "companyDescriptionCk": {
      "type": "customField",
      "options": {
        "preset": "standard"
      },
      "customField": "plugin::ckeditor.CKEditor"
    },
    "companyPlan": {
      "type": "component",
      "repeatable": true,
      "component": "company.plan"
    },
    "rankAdj": {
      "type": "decimal"
    },
    "pageTitle": {
      "type": "string"
    },
    "metaDescription": {
      "type": "text"
    },
    "openSourceType": {
      "type": "enumeration",
      "enum": [
        "GNU General Public",
        "Apache Software Foundation",
        "MIT",
        "AGPL"
      ]
    },
    "companyIcon": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "Employees": {
      "type": "integer"
    },
    "featureItem": {
      "displayName": "Feature",
      "type": "component",
      "repeatable": true,
      "component": "company.feature"
    },
    "technologies_used": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::technology.technology",
      "mappedBy": "company_using"
    },
    "technology_used_by": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::technology.technology",
      "mappedBy": "company_has"
    },
    "primary_category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product-category.product-category",
      "inversedBy": "primary_category_companies"
    },
    "trendRank": {
      "type": "decimal"
    },
    "owner": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "companies"
    },
    "pricingTier": {
      "type": "string"
    }
  }
}
