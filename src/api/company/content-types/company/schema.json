{
  "kind": "collectionType",
  "collectionName": "companies",
  "info": {
    "singularName": "company",
    "pluralName": "companies",
    "displayName": "Company",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "companyName": {
      "type": "string"
    },
    "companyDescription": {
      "type": "richtext"
    },
    "companyUrl": {
      "type": "string"
    },
    "product_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product-category.product-category",
      "inversedBy": "companies"
    },
    "slug": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "twitterHandle": {
      "type": "string",
      "unique": true
    },
    "linkedinHandle": {
      "type": "string"
    },
    "parentCompany": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::company.company",
      "mappedBy": "subsidiaries"
    },
    "subsidiaries": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::company.company",
      "inversedBy": "parentCompany"
    },
    "features": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::feature.feature",
      "inversedBy": "companies"
    },
    "entryPriceMonthly": {
      "type": "decimal"
    },
    "apiPriceMonthly": {
      "type": "decimal"
    },
    "yearFounded": {
      "type": "integer"
    },
    "Headquarters": {
      "type": "string"
    },
    "webScreenshot": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "freeTrial": {
      "type": "boolean"
    },
    "freePricingTier": {
      "type": "boolean"
    }
  }
}
