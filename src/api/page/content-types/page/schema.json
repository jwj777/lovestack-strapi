{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Page",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Headline": {
      "type": "richtext"
    },
    "Subheading": {
      "type": "richtext"
    },
    "Body": {
      "type": "richtext"
    },
    "slug": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "bodyCk": {
      "type": "customField",
      "options": {
        "preset": "standard"
      },
      "customField": "plugin::ckeditor.CKEditor"
    },
    "pageTitle": {
      "type": "string"
    },
    "metaDescription": {
      "type": "text"
    },
    "pageStyle": {
      "type": "enumeration",
      "enum": [
        "Defualt",
        "Display"
      ]
    },
    "mainImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "titleLabel": {
      "type": "string"
    },
    "sectionDz": {
      "type": "dynamiczone",
      "components": [
        "section.section-two-column",
        "section.section-big-text",
        "section.text-cards"
      ]
    },
    "headlineStyle": {
      "type": "enumeration",
      "enum": [
        "Extra Large",
        "Double XL"
      ]
    },
    "fullBackgroundImage": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
